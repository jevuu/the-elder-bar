<GuiXml>
    <Controls>
        <Control name="TEBXMLHeader" virtual = "true">
            <Dimensions y="22" />
            <Controls>
                <Label name="$(parent)Label" font="ZoFontHeader2" color="INTERFACE_COLOR_TYPE_TEXT_COLORS:INTERFACE_TEXT_COLOR_SELECTED" modifyTextType="UPPERCASE">
                    <Anchor point="LEFT" />
                </Label>
            </Controls>
        </Control>
        <!--##########################################-->
        <!-- Tooltip                                  -->
        <!--##########################################-->
        <TopLevelControl name="TEBTooltip" mouseEnabled="true" movable="true" clampedToScreen="true">
            <Dimensions x="410" y="300" />
            <Anchor point="TOPLEFT" />
            <Controls>
                <Backdrop name="$(parent)BG" alpha="1" edge="0" inherits="ZO_DefaultBackdrop" />
                <Label name="$(parent)Left" font="ZoFontChat" color="cccccc" text="">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="0" offsetY="0" />
                </Label>
                <Label name="$(parent)Right" font="ZoFontChat" color="cccccc" text="" horizontalAlignment="RIGHT">
                    <Anchor point="TOPRIGHT" relativeTo="$(parent)" relativePoint="TOPRIGHT" offsetX="0" offsetY="0" />
                </Label>
            </Controls>
        </TopLevelControl>
        <TopLevelControl name="TEBTop" mouseEnabled="true" movable="true" clampedToScreen="true">
            <Anchor point="TOP" relativePoint="TOP" relativeTo="GuiRoot" />
            <OnMoveStart>
                TEBTop:SetDrawLayer(4)
            </OnMoveStart>
            <OnMoveStop>
                TEB.StopMovingBar(self)
            </OnMoveStop>
            <OnUpdate>
                TEB.OnUpdate()
            </OnUpdate>
            <Controls>
                <Backdrop name="$(parent)BG" alpha="1" edge="0">
                    <Anchor point="TOPLEFT" relativeTo="TEBTop" offsetX="-ZO_DEFAULT_BACKDROP_ANCHOR_OFFSET" offsetY="-ZO_DEFAULT_BACKDROP_ANCHOR_OFFSET" />
                    <Anchor point="BOTTOMRIGHT" relativeTo="TEBTop" offsetX="ZO_DEFAULT_BACKDROP_ANCHOR_OFFSET" offsetY="ZO_DEFAULT_BACKDROP_ANCHOR_OFFSET" />

                    <Edge file="EsoUI/Art/Tooltips/UI-Border.dds" edgeFileWidth="128" edgeFileHeight="16" />
                    <Center file="EsoUI/Art/Tooltips/UI-TooltipCenter.dds" />
                    <Insets left="16" top="16" right="-16" bottom="-16" />

                    <Controls>
                        <Texture name="$(parent)MungeOverlay" textureFile="EsoUI/Art/Tooltips/munge_overlay.dds" addressMode="WRAP">
                            <AnchorFill />
                        </Texture>
                    </Controls>
                </Backdrop>
                <Backdrop name="$(parent)CombatBG" alpha="0" edge="0">
                    <Anchor point="TOPLEFT" offsetX="-ZO_DEFAULT_BACKDROP_ANCHOR_OFFSET" offsetY="-ZO_DEFAULT_BACKDROP_ANCHOR_OFFSET" />
                    <Anchor point="BOTTOMRIGHT" offsetX="ZO_DEFAULT_BACKDROP_ANCHOR_OFFSET" offsetY="ZO_DEFAULT_BACKDROP_ANCHOR_OFFSET" />

                    <Edge file="TEB/Images/combat-border.dds" edgeFileWidth="128" edgeFileHeight="16" />
                    <Center file="TEB/Images/combat-center.dds" />
                    <Insets left="16" top="16" right="-16" bottom="-16" />

                    <Controls>
                        <Texture name="$(parent)MungeOverlay" textureFile="EsoUI/Art/Tooltips/munge_overlay.dds" addressMode="WRAP">
                            <AnchorFill />
                        </Texture>
                    </Controls>
                </Backdrop>

                <!--##########################################-->
                <!-- Information Anchor                       -->
                <!--##########################################-->
                <Button name="$(parent)InfoAnchor">
                    <Dimensions x="0" y="22" />
                    <Anchor point="LEFT" relativePoint="LEFT" offsetX="0" offsetY="0" />
                </Button>

                <!--##########################################-->
                <!-- Latency                                  -->
                <!--##########################################-->
                <Button name="$(parent)LatencyIcon" mouseEnabled="true" movable="true" clampedToScreen="true">
                    <Dimensions x="22" y="22" />
                    <Anchor point="LEFT" relativeTo="$(parent)InfoAnchor" relativePoint="RIGHT" offsetX="0" offsetY="0" />
                    <Textures normal="TEB/Images/latency_white.dds" />
                    <OnMoveStart>
